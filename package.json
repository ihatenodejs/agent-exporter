{
  "name": "agent-exporter",
  "version": "1.0.1",
  "description": "Export usage statistics from various agents",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "agent-exporter": "./dist/agent-exporter"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/cli.ts --compile --minify --outfile dist/agent-exporter",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "link": "bun link",
    "start": "bun dist/agent-exporter",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "ai",
    "usage",
    "tracking",
    "opencode",
    "ccusage",
    "claude-code",
    "codex",
    "export",
    "claude",
    "anthropic",
    "openai",
    "gemini",
    "qwen code"
  ],
  "author": "ihatenodejs",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihatenodejs/agent-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/ihatenodejs/agent-exporter/issues"
  },
  "homepage": "https://github.com/ihatenodejs/agent-exporter#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@pydantic/genai-prices": "^0.0.35",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dayjs": "^1.11.18",
    "ink": "^6.4.0",
    "react": "^19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.4.0",
    "react-devtools-core": "^6.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
